<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SignUpServlet</servlet-name>
  	<servlet-class>com.feedbackApplication.database.SignUpServlet</servlet-class>
 </servlet>
  
   <servlet-mapping>
   	     <servlet-name>SignUpServlet</servlet-name>
  		<url-pattern>/SignUpServlet</url-pattern>
   </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.feedbackApplication.database.LoginServlet</servlet-class>
 </servlet>
  
   <servlet-mapping>
   	     <servlet-name>LoginServlet</servlet-name>
  		<url-pattern>/LoginServlet</url-pattern>
   </servlet-mapping>
   
    <servlet>
  	<servlet-name>GiveFeedbackServlet</servlet-name>
  	<servlet-class>com.feedbackApplication.database.GiveFeedbackServlet</servlet-class>
 </servlet>
  
   <servlet-mapping>
   	     <servlet-name>GiveFeedbackServlet</servlet-name>
  		<url-pattern>/GiveFeedbackServlet</url-pattern>
   </servlet-mapping>
   
     <servlet>
  	<servlet-name>ViewFeedbackServlet</servlet-name>
  	<servlet-class>com.feedbackApplication.database.ViewFeedbackServlet</servlet-class>
 </servlet>
  
   <servlet-mapping>
   	     <servlet-name>ViewFeedbackServlet</servlet-name>
  		<url-pattern>/ViewFeedbackServlet</url-pattern>
   </servlet-mapping>
  
</web-app>
